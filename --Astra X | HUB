
local Library = loadstring(game:HttpGet("https://raw.githubusercontent.com/xHeptc/Kavo-UI-Library/main/source.lua"))()

--Tabs and Section
local Window = Library.CreateLib("Astra X | HUB", "Serpent")
local MainTab = Window:NewTab("Main")
local PlayerTab = Window:NewTab("Player")
local UniversalScriptsTab = Window:NewTab("UniversalScripts")
local GameScriptsTab = Window:NewTab("GameScripts")
local OtherTab = Window:NewTab("Other")
local CreditsTab = Window:NewTab("Credits")
local MainSection = MainTab:NewSection("Main")
local PlayerSection = PlayerTab:NewSection("Player")
local UniversalScriptsSection = UniversalScriptsTab:NewSection("UniversalScripts")
local GameScriptsSection = GameScriptsTab:NewSection("GameScripts")
local OtherSection = OtherTab:NewSection("Other")
local CreditsSection = CreditsTab:NewSection("Credits")

--MainTab
MainSection:NewKeybind("ToggleUI", "ToggleUIInfo", Enum.KeyCode.F, function()
	Library:ToggleUI()
end)
MainSection:NewButton("BTools", "BTools", function()
    loadstring(game:HttpGet("https://cdn.wearedevs.net/scripts/BTools.txt"))()
end)

--PlayerTab

